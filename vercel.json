{
  "cleanUrls": true,
  "trailingSlash": false,

  "rewrites": [
    { "source": "/merch", "destination": "/merch.html" },
    { "source": "/merch.html", "destination": "/merch.html" },
    { "source": "/thankyou", "destination": "/thankyou.html" },
    { "source": "/thankyou.html", "destination": "/thankyou.html" }
  ],

  "crons": [
    { "path": "/api/cron/post", "schedule": "0 14 * * *" },
    { "path": "/api/cron/post", "schedule": "0 1 * * *" }
  ]
}
